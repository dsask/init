#These settings work on Ubuntu 10.04 with a custom compiled urxvt
# urxvt compiled with:
#    --enable-256-color
#    --enable-xft

URxvt.perl-ext: matcher
URxvt.urlLauncher: firefox
URxvt.matcher.button: 1
URxvt.background: black
URxvt.foreground: white
URxvt.font: xft:Monospace:pixelsize=14
URxvt.scrollBar: False
URxvt.letterSpace: -2
URxvt.cursorBlink: True
URxvt.iconFile: /usr/share/icons/Humanity/apps/48/utilities-terminal.svg
